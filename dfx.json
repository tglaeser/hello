{
  "canisters": {
    "hello": {
      "main": "src/hello/main.mo",
      "type": "motoko"
    },
    "hello_assets": {
      "dependencies": [
        "hello"
      ],
      "frontend": {
        "entrypoint": "src/hello_assets/public/index.js",
        "output": "canisters/hello_assets/assets"
      },
      "source": [
        "src/hello_assets/assets"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "output": "canisters/",
      "packtool": ""
    }
  },
  "dfx": "0.5.11",
  "networks": {
    "local": {
      "bind": "0.0.0.0:8000"
    },
    "tungsten": {
      "providers": [
        "https://gw.dfinity.network"
      ]
    }
  },
  "version": 1
}